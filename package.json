{
  "name": "gatos",
  "version": "1.0.0",
  "scripts": {
    "dev": "cd packages/website && npm run dev",
    "test:integration": "pnpm --filter playwright test",
    "test:install": "pnpm --filter playwright playwright install",
    "docs": "cd packages/documentation && npm run start",
    "fmt": "prettier --write 'packages/website/src/**/*.{js,jsx,ts,tsx}'"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "private": true,
  "packageManager": "pnpm@7.25.0",
  "dependencies": {
    "prettier": "^2.8.3"
  }
}
